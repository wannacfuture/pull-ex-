{
  "name": "jahe-react-starter",
  "version": "1.0.0",
  "description": "React Starter Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/jahe/jahe-react-starter.git"
  },
  "scripts": {
    "start": "node devserver.js",
    "build:clean": "rimraf ./public/dist",
    "build:prod": "npm run build:clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --profile --json > webpack-stats.json",
    "test": "jest --coverage",
    "watch:test": "jest --watch",
    "manage:translations": "babel-node ./translationRunner.js"
  },
  "author": "Jannik Hell",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.4.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^1.3.0",
    "react-intl-translations-manager": "^3.1.0",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "json-loader": "^0.5.4",
    "moment": "^2.15.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/client/**/*.js"
    ]
  }
}
